{
  "version": 2,
  "builds": [
    { "src": "dist/spa/**", "use": "@vercel/static" },
    { "src": "dist/server/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/server/(.*)", "dest": "/dist/server/$1" },
    { "src": "/assets/(.*)", "dest": "/dist/spa/assets/$1" },
    { "src": "/favicon.ico", "dest": "/dist/spa/favicon.ico" },
    { "src": "/(.*)", "dest": "/dist/spa/index.html" }
  ]
}
